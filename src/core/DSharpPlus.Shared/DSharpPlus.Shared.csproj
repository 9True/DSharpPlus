<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <IsPublicFacing>true</IsPublicFacing>
    <Description>$(Description) This package contains shared types between public-facing and internal implementation packages.</Description>
    <OutputType>Library</OutputType>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsAotCompatible>true</IsAotCompatible>
    <NoWarn>$(NoWarn);CA1008</NoWarn>
    
    <RootNamespace>DSharpPlus</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommunityToolkit.HighPerformance" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\lib\bundles\src\Bundles\Bundles.csproj" />
  </ItemGroup>

</Project>
